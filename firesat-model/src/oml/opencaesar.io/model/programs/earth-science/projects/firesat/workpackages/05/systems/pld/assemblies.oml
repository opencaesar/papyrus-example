@dc:title "Payload Assemblies"
description <http://opencaesar.io/model/programs/earth-science/projects/firesat/workpackages/05/systems/pld/assemblies> with # as pld-assemblies {

	uses <http://imce.jpl.nasa.gov/discipline/fse/fse>

	extends <http://opencaesar.io/model/programs/earth-science/projects/firesat/systems/pld/pld>

	ref ci pld:Payload [
		base:aggregates PayloadModule
		base:aggregates Sensor
	]

	// Payload Module

	@rdfs:label "Payload Module"
	ci PayloadModule : fse:Assembly [
		base:hasIdentifier "540000"
	]

	// Sensor

	@rdfs:label "Sensor"
	ci Sensor : fse:Assembly [
		base:hasIdentifier "541000"
	]

}